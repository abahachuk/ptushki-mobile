{
  "name": "PtushkiMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint ./src/**/*.js App.js",
    "lint:fix": "eslint ./src/**/*.js App.js --fix",
    "android": "node node_modules/react-native/local-cli/cli.js run-android --appIdSuffix 'debug' -- --reset-cache",
    "android-clean-osx": "cd android && ./gradlew clean && cd ..",
    "android-restart-osx": "npm run android-clean-osx && npm run android",
    "build": "npm run android-restart-osx",
    "build-android-pr": "cd android && ./gradlew assembleRelease && cd ..",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@babel/runtime": "7.6.2",
    "@react-native-community/async-storage": "1.7.1",
    "EventEmitter": "1.0.0",
    "i18n-js": "3.2.1",
    "prop-types": "15.7.2",
    "ramda": "0.26.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-elements": "1.2.7",
    "react-native-gesture-handler": "1.5.2",
    "react-native-image-picker": "1.1.0",
    "react-native-modalbox": "2.0.0",
    "react-native-reanimated": "1.4.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "4.0.10",
    "react-navigation-drawer": "2.3.3",
    "react-navigation-stack": "1.10.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@react-native-community/eslint-config": "0.0.3",
    "@babel/plugin-transform-runtime": "7.6.2",
    "babel-jest": "24.9.0",
    "eslint": "5.15.3",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "3.0.8",
    "jest": "24.9.0",
    "jetifier": "1.6.4",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "1.16.4",
    "react-test-renderer": "16.9.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint:fix"
    }
  }
}
